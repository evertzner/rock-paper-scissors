{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:css/standard"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "css"],
	"rules": {
		"react/prop-types": "off",
		"no-console": 1,
		"no-inline-comments": 1
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
